{
    "name": "ivyhjk/autobake",
    "description": "Autobake plugin for CakePHP",
    "keywords": ["framework", "cakephp", "plugin", "bake", "autobake"],
    "type": "cakephp-plugin",
    "require": {
        "php": ">=5.4.16",
        "cakephp/cakephp": "~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "Autobake\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Autobake\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    },
    "minimum-stability": "dev"
}
